r;\n      transition: color 400ms ease;\n      &:hover {\n         color: #f5f5f7;\n      }\n   }\n}\n\n.nav-sticky {\n   width: 100%;\n   position: sticky;\n   top: 0;\n   z-index: 999;\n   padding: 15px 0;\n   border-bottom: 1px solid #ccc;\n   font-size: 12px;\n   font-weight: 400;\n   background: rgba(255,255,255,0.7);\n   backdrop-filter: saturate(180%) blur(20px);\n   .container {\n      max-width: 1000px;\n      display: flex;\n      justify-content: space-between;\n      align-items: flex-end;\n      margin: 0 auto;\n      .left p {\n         &:first-child {\n            color: rgba(0, 0, 0, 0.6);\n         }\n         &:last-child {\n            font-size: 21px;\n            line-height: 23px;\n            font-weight: 600;\n            color: rgba(0, 0, 0, 0.9);\n         }\n      }\n      .right {\n         display: flex;\n         align-items: center;\n         li {\n            margin-right: 20px;\n            color: rgba(0, 0, 0, 0.9);\n            cursor: pointer;\n            &:hover {\n               color: $apple-blue;\n            }\n            &:first-child {\n               color: rgba(0, 0, 0, 0.5);\n            }\n         }\n      }\n      button {\n         background: #0071e3;\n         color: rgba(255, 255, 255, 0.9);\n         border: none;\n         border-radius: 999px;\n         padding: 4px 11px;\n         cursor: pointer;\n         &:hover {\n            background: #1d7ce5;\n         }\n      }\n   }\n}\n","scss/_variables.scss":"$text-color: #1d1d1f;\n$apple-blue: #06c;","scss/_hero.scss":"@import 'variables';\n\n.hero {\n   .img-container {\n      max-width: 1440px;\n      margin: 0 auto;\n      img {\n         width: 100%;\n         object-fit: cover;\n      }\n   }\n   .text-info {\n      max-width: 1000px;\n      color: $text-color;\n      font-weight: 600;\n      text-align: center;\n      margin: 0 auto;\n      h1 {\n         font-size: 96px;\n         margin-bottom: 10px;\n      }\n      p {\n         max-width: 840px;\n         font-size: 32px;\n         line-height: 37px;\n         margin: 0 auto 40px;\n      }\n      .price {\n         display: block;\n         font-size: 21px;\n         margin-bottom: 35px;\n      }\n      .cto {\n         display: flex;\n         justify-content: center;\n         a {\n            color: $apple-blue;\n            font-size: 21px;\n            line-height: 21px;\n            font-weight: 600;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            &:first-child {\n               margin-right: 30px;\n            }\n            svg {\n               margin-left: 7px;\n            }\n            &:hover span {\n               text-decoration: underline;\n            }\n         }\n      }\n   }\n}\n","scss/_animation.scss":".animated-macbook {\n   max-width: 1336px;\n   margin: 0 auto;\n   height: 350vh;\n   position: relative;\n   margin-bottom: 200px;\n   canvas {\n      width: 100%;\n      aspect-ratio: 1336/786;\n      position: sticky;\n      top: 90px;\n   }\n}\n\n.figures-wp {\n   position: sticky;\n   top: 90px;\n   width: 100%;\n   aspect-ratio: 1336/786;\n   .figure {\n      position: absolute;\n      .sep {\n         display: block;\n         width: 40px;\n         height: 4px;\n         background: #000;\n         opacity: 0;\n         transform-origin: left;\n         transform: scale(0);\n         transition: transform 0.4s ease-out, opacity 0.4s ease-out;\n      }\n\n      .up-to {\n         display: block;\n         font-weight: 500;\n         line-height: 1.2;\n         font-size: 17px;\n         margin-top: 14px;\n         color: #86868b;\n         transform: translate3d(0, 150%, 0);\n         opacity: 0;\n         transition: transform 0.5s ease-out, opacity 0.5s ease-out;\n      }\n      .label-wp {\n         overflow: hidden;\n         .label {\n            display: flex;\n            align-items: flex-end;\n            .left {\n               color: #1d1d1f;\n               font-size: 100px;\n               font-weight: 600;\n               line-height: 1.05;\n               transform: translate3d(0, 130%, 0);\n               transition: transform 0.4s ease-out;\n            }\n\n            .right {\n               font-weight: 500;\n               font-size: 17px;\n               color: #86868b;\n               margin-left: 12px;\n               margin-bottom: 12px;\n               max-width: 105px;\n               transform: translate3d(0, 130%, 0);\n               transition: transform 0.4s ease-out;\n            }\n         }\n      }\n      &.active {\n         .sep {\n            opacity: 1;\n            transform: scale(1);\n         }\n         .up-to {\n            transform: translate3d(0, 0, 0);\n            opacity: 1;\n         }\n         .label-wp {\n            .label {\n               .left {\n                  transition: transform 0.4s ease-out 200ms;\n                  transform: translate3d(0, 0, 0);\n               }\n               .right {\n                  transition: transform 0.4s ease-out 350ms;\n                  transform: translate3d(0, 0, 0);\n               }\n            }\n         }\n      }\n   }\n}\n\n.animated-macbook-closed {\n   width: 100%;\n   overflow: hidden;\n   display: flex;\n   justify-content: center;\n   padding-bottom: 214px;\n   .left-side {\n      position: relative;\n      left: -275px;\n      display: block;\n      margin-right: 75px;\n   }\n   .right-side {\n      position: relative;\n      right: -275px;\n      display: block;\n      margin-left: 75px;\n   }\n}\n","scss/media.scss":"@import 'variables';\n\n.small-screen {\n   display: none;\n   color: $text-color;\n   font-weight: 600;\n   text-align: center;\n   font-size: 24px;\n   width: 100vw;\n   height: 100vh;\n   justify-content: center;\n   align-items: center;\n   padding: 40px;\n}\nfooter {\n   color: rgba(0, 0, 0, 0.6);\n   font-size: 12px;\n   font-weight: 400;\n   text-align: center;\n   padding: 10px;\n   a {\n      color: #0071e3;\n   }\n}\n\n@media screen and (max-width: 1055px) {\n   .nav-fixed,\n   .nav-sticky,\n   .hero,\n   .animated-macbook,\n   .animated-macbook-closed {\n      display: none;\n   }\n   .small-screen {\n      display: flex;\n   }\n}\n"},"lineCount":null}},"error":null,"hash":"bfb84249bff3e347652f2323cc0d6652","cacheData":{"env":{}}}